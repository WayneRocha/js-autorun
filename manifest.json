{
	"manifest_version": 3,
	"name": "JS AutoRun",
	"description": "Run JavaScript automaticaly",
	"version": "1.1",
	"icons": {
		"128": "128.png"
	},
	"action": {
		"default_popup": "index.html"
	},
	"permissions": ["scripting", "storage"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["runner.js"]
		}
	]
}
